{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fengyuanchen/cropper",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/fengyuanchen/cropper/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/div",
                "version": "4.10.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/div"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/div/releases/div_4.10.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/indentblock",
                "version": "4.16.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor/plugins/indentblock"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/indentblock/releases/indentblock_4.16.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/simplecolor",
                "version": "1.2.3",
                "dist": {
                    "url": "https://github.com/recurser/jquery-simple-color/archive/v1.2.3.zip",
                    "type": "zip"
                },
                "type": "drupal-library"
            }
        }
    ],
    "require": {
        "bower-asset/lazysizes": "^5.1",
        "ckeditor/div": "4.10.1",
        "ckeditor/indentblock": "4.16.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor_accordion": "^2.0",
        "drupal/ckeditor_indentblock": "^1.0@beta",
        "drupal/color_field": "^3.0",
        "drupal/config_split": "^1.7",
        "drupal/content_lock": "^2.2",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/crop": "^2.1",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/entity_clone": "^2.1@beta",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/field_states_ui": "^3.0",
        "drupal/file_mdm": "^2.1",
        "drupal/geocoder": "^4.23",
        "drupal/geolocation": "^3.13",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jsonapi_extras": "^3.25",
        "drupal/jsonapi_hypermedia": "^1.8",
        "drupal/jsonapi_image_styles": "^3.0",
        "drupal/jsonapi_include": "^1.4",
        "drupal/jsonapi_menu_items": "1.2.4",
        "drupal/jsonapi_search_api": "^1.0@RC",
        "drupal/jsonapi_views": "^1.1",
        "drupal/layout_disable": "^2.0",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/layout_section_classes": "^1.0",
        "drupal/linkit": "^6.0",
        "drupal/login_destination": "^2.0@beta",
        "drupal/mailsystem": "^4.5",
        "drupal/menu_block": "^1.7",
        "drupal/menu_item_extras": "^3.0",
        "drupal/menu_position": "^1.0@beta",
        "drupal/metatag": "^2.0",
        "drupal/metatag_import_export_csv": "^1.3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/module_filter": "^4.0",
        "drupal/next": "^1.2",
        "drupal/oembed_lazyload": "^2.0",
        "drupal/paragraph_view_mode": "^2.0",
        "drupal/path_redirect_import": "^2.0",
        "drupal/pathauto": "^1.8",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/quick_node_clone": "^1.15",
        "drupal/rabbit_hole": "^1.0",
        "drupal/real_aes": "^2.6",
        "drupal/redirect": "^1.7",
        "drupal/redirect_after_login": "^2.7",
        "drupal/rest_entity_recursive": "2.0.4-rc4",
        "drupal/restui": "^1.21",
        "drupal/s3fs": "^3.6",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.34",
        "drupal/smtp": "^1.2",
        "drupal/svg_image": "^3.1",
        "drupal/svg_image_widget_crop": "^1.0",
        "drupal/tamper": "^1.0@alpha",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.0",
        "drupal/webform": "^6.1",
        "drupal/webform_rest": "^4.0",
        "drupal/weight": "^3.3",
        "drupal/xmlsitemap": "^1.5",
        "drush/drush": "^12.5",
        "fengyuanchen/cropper": "dev-master",
        "furf/jquery-ui-touch-punch": "dev-master",
        "geocoder-php/google-maps-provider": "^4.7",
        "jquery/simplecolor": "1.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "vlucas/phpdotenv": "^5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youï¿½ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
            "drupal/subrequests": {
                "Get same results on different request": "https://www.drupal.org/files/issues/2019-07-18/change_request_type-63049395-09.patch"
            },
            "drupal/decoupled_router": {
                "Unable to resolve path on node in other language than default": "https://www.drupal.org/files/issues/2024-07-11/decouple_router-3111456-resolve-language-issue-63--get-translation-re-rolled.patch"
            },
            "drupal/core": {
                "Link fields": "https://www.drupal.org/files/issues/2022-03-04/3066751-68.patch",
                "Claro Theme Patch" :"https://www.drupal.org/files/issues/2022-09-28/3312270-claro-menu-twig-fix.patch",
                "Media Library issue 3345064": "https://www.drupal.org/files/issues/2023-02-28/3345064.patch",
                "Permissions errors": "https://www.drupal.org/files/issues/2024-06-03/3358586-36.patch"
            },
            "drupal/jsonapi_menu_items":{
                "fix fields not showing in menu": "https://www.drupal.org/files/issues/2022-08-01/3276561-05-menu_item_extras_support.patch"
            },
            "drupal/rest_entity_recursive":{
                "Fix for ReferenceItemNormalizer class": "./patches/rest_entity_recursive_normalizer.patch"
            },
            "drupal/quick_node_clone":{
                "Issue number 3218222": "https://www.drupal.org/files/issues/2022-04-26/quick_node_clone-n3218222-14.patch"
            },
            "drupal/jsonapi_image_styles": {
                "S3 url build": "https://www.drupal.org/files/issues/2024-08-20/Issue-3469284-The-jsonapi_image_styles-module-genera.patch"
            },
            "drupal/content_type_clone": {
                "Drupal 10 compatability": "https://www.drupal.org/files/issues/2024-07-21/content_type_clone.8.x-1.1.rector.patch"
            }
        }
    },
    "require-dev": {
        "palantirnet/drupal-rector": "^0.18.2"
    }
}
